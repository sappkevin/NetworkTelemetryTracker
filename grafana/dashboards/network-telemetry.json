      "targets": [
        {
          "query": "// Success (2xx)\nsuccess = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"http_status_code\")\n  |> filter(fn: (r) => r._value >= 200 and r._value < 300)\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"Success (2xx)\"}))\n\n// Client Error (4xx)\nclient_error = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"http_status_code\")\n  |> filter(fn: (r) => r._value >= 400 and r._value < 500)\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"Client Error (4xx)\"}))\n\n// Server Error (5xx)\nserver_error = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"http_status_code\")\n  |> filter(fn: (r) => r._value >= 500 and r._value < 600)\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"Server Error (5xx)\"}))\n\nunion(tables: [success, client_error, server_error])",
          "refId": "A"
        }
      ],
