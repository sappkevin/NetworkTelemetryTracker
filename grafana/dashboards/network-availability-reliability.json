      "targets": [
        {
          "query": "// Calculate MTBF (Mean Time Between Failures)\n// Based on total uptime divided by number of failures\ntotal_measurements = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_available\")\n  |> count()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nfailures = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_available\")\n  |> filter(fn: (r) => r._value == 0)\n  |> count()\n  |> findRecord(fn: (key) => true, idx: 0)\n\n// MTBF = Total operating time / Number of failures\n// Assuming 5-minute intervals, convert to minutes\nmtbf_minutes = if exists failures._value and failures._value > 0 then\n  float(v: total_measurements._value) * 5.0 / float(v: failures._value)\nelse\n  1440.0  // 24 hours default if no failures\n\nfrom(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_available\")\n  |> last()\n  |> map(fn: (r) => ({r with _value: mtbf_minutes}))",
          "refId": "A"
        }
      ],
      "title": "MTBF (Mean Time Between Failures)",
      "type": "stat"
    },
