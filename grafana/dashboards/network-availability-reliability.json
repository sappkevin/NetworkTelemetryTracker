      "targets": [
        {
          "query": "// Available (service up and not degraded)\navailable = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_available\" or r._field == \"service_degraded\")\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({r with _value: if r.service_available == 1 and r.service_degraded == 0 then 1 else 0}))\n  |> sum()\n  |> map(fn: (r) => ({r with _field: \"Available\"}))\n\n// Degraded (service up but degraded)\ndegraded = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_degraded\")\n  |> filter(fn: (r) => r._value == 1)\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"Degraded\"}))\n\n// Unavailable (service down)\nunavailable = from(bucket: \"default\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"network_telemetry\")\n  |> filter(fn: (r) => r._field == \"service_available\")\n  |> filter(fn: (r) => r._value == 0)\n  |> count()\n  |> map(fn: (r) => ({r with _field: \"Unavailable\"}))\n\nunion(tables: [available, degraded, unavailable])",
