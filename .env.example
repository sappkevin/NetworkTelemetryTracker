# Network Telemetry Service Configuration Template
# Copy this file to .env and customize the values

# Network Monitoring Configuration
TARGET_FQDN=google.com
MONITORING_INTERVAL=60
PING_COUNT=4
PING_TIMEOUT=5

# InfluxDB Configuration
INFLUXDB_URL=http://influxdb:8086
INFLUXDB_ORG=network_telemetry
INFLUXDB_BUCKET=metrics
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=admin123
INFLUXDB_TOKEN=your-admin-token-here

# Logging Configuration
LOG_LEVEL=INFO

# Google OAuth Configuration (for Grafana 2FA)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# Grafana Security Configuration
GF_SECURITY_ADMIN_PASSWORD=admin123!
GF_SECURITY_SECRET_KEY=your-long-random-secret-key-here